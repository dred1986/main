{
  "automock": true,
  "roots": [
    "<rootDir>"
  ],
  "globals": {
    "__DEV__": true
  },
  "transform": {
    "^.+\\.js$": "babel-jest"
  },
  "setupFiles": [
    "<rootDir>/setupEnv.js"
  ],
  "moduleFileExtensions": [
    "js",
    "json"
  ],
  "setupFilesAfterEnv": ["<rootDir>/testSetupFile.js"],
  "testRegex": "/__tests__/.*\\-test\\.(es6|js)$",
  "modulePathIgnorePatterns": [
    "/tmp/",
    "/node_modules/",
    "/.module-cache/"
  ],
  "timers": "fake",
  "unmockedModulePathPatterns": [
    "./src/Mixin",
    "./src/Util",
    "babel-runtime",
    "classnames",
    "react",
    "react-dom",
    "react-addons-test-utils",
    "fbjs"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/"
  ]
}
